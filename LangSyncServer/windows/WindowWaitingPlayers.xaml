<Window x:Class="LangSyncServer.windows.WindowWaitingPlayers"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LangSyncServer.windows"
        mc:Ignorable="d"
        Title="Waiting players" Height="450" Width="800"
        Icon="/assets/logo.png"
        WindowStartupLocation="CenterScreen"
        >
    <Grid>
        <Frame x:Name="content" Content="Frame" NavigationUIVisibility="Hidden" Margin="0,0,0,70"/>
        <Button x:Name="primaryBtn" Content="Start game" Style="{StaticResource primaryButton}" Height="40" Width="100" HorizontalAlignment="Center" Margin="0,0,0,15" VerticalAlignment="Bottom" Click="Button_Click_1"/>


    </Grid>
</Window>
